{
    "manifest_version": 3,
    "version": "1.1",
    "name": "Leads Tracker",
    "action": {
        "default_popup": "index.html",
        "default_icon": "icon.png"
    },
    "permissions": ["tabs", "activeTab", "storage"],
    "host_permissions": ["<all_urls>"],
    "background": {
        "service_worker": "background.js"
    }
}
